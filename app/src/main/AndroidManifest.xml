<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.binioter">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/swipeback_activity_style">
    <activity
      android:name="com.binioter.ui.MainActivity"
      android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name="com.binioter.base.BaseActivity"
      android:label="@string/title_activity_base"
      android:screenOrientation="portrait" />
    <activity
      android:name="com.binioter.ui.CommonDialogActivity"
      android:label="@string/title_activity_dialog" />
    <activity
      android:name="com.binioter.ui.FlowLayoutActivity"
      android:label="@string/title_activity_flow" />
    <activity
      android:name="com.binioter.ui.TipTextViewActivity"
      android:label="@string/title_activity_tips" />
  </application>

</manifest>
